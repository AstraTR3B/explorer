.panel.panel-default
  .panel-heading
    h3.panel-title #{settings.locale.rl_received_coins} 
  table.table.table-hover.table-bordered(cellspacing="0")
    thead
      tr
        th(style='text-align:left;') 
          span.fa.fa-flag-checkered
        th(style='text-align:left;')  #{settings.locale.tx_address} 
        th(style='text-align:left;').hidden-xs #{settings.locale.rl_received} (#{settings.symbol})
    tbody
      - var count = 0
      each item in received
        - count = count + 1
        - var itemFixed = item.received / 100000000
        tr
          td(style='text-align:left;') 
            =count
          td(style='text-align:left;') 
            a(href='/address/#{item.a_id}') #{item.a_id}
            include ./rl_labels.jade
          td(style='text-align:left;').hidden-xs
            =itemFixed.toFixed(8)
